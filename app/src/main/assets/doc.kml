      <Placemark>
         <name>From Colts Neck Naval Pier, New Jersey I</name>
         <styleUrl>#cwmds</styleUrl>
         <LookAt>
            <longitude>-71.026667</longitude>
            <latitude>39.545</latitude>
            <altitude>0</altitude>
            <range>400000</range>
            <tilt>0</tilt>
