      <Placemark>
         <name>East of Cape Moreton, Australia</name>
         <styleUrl>#cwmds</styleUrl>
         <LookAt>
            <longitude>153.861898</longitude>
            <latitude>-27.02853</latitude>
            <altitude>0</altitude>
