      <Placemark>
         <name>East of Cape Kennedy, Florida</name>
         <styleUrl>#cwmds</styleUrl>
         <LookAt>
            <longitude>-76</longitude>
            <latitude>29.35</latitude>
            <altitude>0</altitude>
            <range>400000</range>
            <tilt>0</tilt>
